#
# wm independent hotkeys
#

# terminal emulator
super + Return
        kitty

# program launcher
super + p
        rofi -show run
        # dmenu_run

#audio
XF86AudioLowerVolume
	amixer set Master 5%-

XF86AudioRaiseVolume
	amixer set Master 5%+

XF86AudioMute
    amixer -q -D pulse sset Master toggle


super + shift + q
    killall xinit
